<div class="card match123" style="width: 15rem;">
  {{#each profile}}

  <img class="card-img-top picformat" src="{{this.photo}}" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title">{{this.first_name}} {{this.last_name}}</h5>
    <p class="card-text">Match</p>
    {{!-- <a href="#" class="btn btn-primary">Go somewhere</a> --}}
    <button class="btn btn-success" data-profilematchid="{{this.id}}" data-pactivity="{{this.primary_training_type}}"
      class="gitfit-match">Accept</button>
    <button class="btn btn-dark" data-profilematchid="{{this.id}}" data-pactivity="{{this.primary_training_type}}"
      class="gitfit-match">Block</button>
  </div> <br>

  {{/each}}
 <br> 
 <br> 
</div>
{{>chat}}
{{!-- Ralph's new stuff above, original is below(comments dont work in handlebars) --}}

<div class="container">
<div class="row">
      {{#each matchLimit}}
  <div class="col">
    <div class="card match123 d-inline-block" style="width: 12rem;">
  
      {{!-- <div class="row" >
  <div class="col-sm-6"> --}}
      <div class="d-inline-block">
        {{!-- style= "display-inline: auto" --}}
