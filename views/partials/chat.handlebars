

<div id="messageArea">
    <div class="row">
      <div class="col-md-4">
        <div class="well onlineUsers">
          <h4 id="txtcolor">Online Users</h4>
          <ul class="list-group-item" id="users">
            {{#each profile}}
             <li>{{first_name}}</li>
            {{/each}}
          </ul>
        </div>
      </div>
      <div class="col-md-8">
        <div class="chat" id="chat" ></div>

          <form id="messageForm">
            <div id="chatBody"></div>
          <div class="form-group">
            <!-- <label>Enter a Message</label> -->
            <input type="text" class="form-control" id="message" placeholder="Start Message">
            <br>
            <input type="submit" class="btn btn-primary" value="Send Message">
          </div>
          </form>
        </div>
      </div>
    </div>
